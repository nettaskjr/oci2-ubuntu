# Renomeie este arquivo para terraform.tfvars e preencha os valores
tenancy_ocid         = "ocid1.tenancy.oc1..xxxxxxxx"
user_ocid            = "ocid1.user.oc1..xxxxxxxx"
compartment_ocid     = "ocid1.compartment.oc1..xxxxxxxx"
fingerprint          = "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
ssh_public_key_path  = "/home/usuario/.ssh/id_rsa.pub"
api_private_key_path = "/home/usuario/.oci/oci_api_key.pem"
# Cloudflare
cloudflare_api_token = "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
# Backend S3 (Nota: estas variáveis não configuram o backend automaticamente, 
# o backend deve ser configurado no bloco terraform ou via CLI, 
# mas mantemos aqui para referência se usar scripts de wrapper)
state_bucket_name = "terraform-state-xxxxxxxx"
# webhook do discord para envio de informacoes
discord_webhook_url = "https://discordapp.com/api/webhooks/*********"
